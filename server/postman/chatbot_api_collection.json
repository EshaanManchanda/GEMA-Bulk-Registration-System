{
    "info": {
        "_postman_id": "chatbot-api-collection",
        "name": "Chatbot API",
        "description": "API collection for Chatbot public endpoints including message sending, event data fetching, and FAQ operations.",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Send Message",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "x-api-key",
                        "value": "{{api_key}}",
                        "type": "text"
                    },
                    {
                        "key": "Content-Type",
                        "value": "application/json",
                        "type": "text"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n    \"message\": \"Hello\",\n    \"sessionId\": \"optional-session-id\"\n}"
                },
                "url": {
                    "raw": "{{base_url}}/api/v1/chatbot/message",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "v1",
                        "chatbot",
                        "message"
                    ]
                }
            },
            "response": []
        },
        {
            "name": "Get Event Data",
            "request": {
                "method": "GET",
                "header": [
                    {
                        "key": "x-api-key",
