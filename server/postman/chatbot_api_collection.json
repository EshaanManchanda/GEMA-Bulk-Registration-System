                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "v1",
                        "chatbot",
                        "faqs"
                    ]
                }
            },
            "response": []
        },
        {
            "name": "Search FAQs",
            "request": {
                "method": "GET",
                "header": [
                    {
                        "key": "x-api-key",
                        "value": "{{api_key}}",
                        "type": "text"
                    }
                ],
                "url": {
                    "raw": "{{base_url}}/api/v1/chatbot/faqs/search?q=event",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "v1",
                        "chatbot",
                        "faqs",
                        "search"
                    ],
                    "query": [
                        {
                            "key": "q",
                            "value": "event"
                        }
                    ]
                }
            },
            "response": []
        },
        {
            "name": "Find Similar FAQs",
