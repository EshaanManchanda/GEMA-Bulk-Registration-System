# API Configuration
VITE_API_URL=http://localhost:5000/api/v1

# Razorpay Configuration (for INR payments)
# Development key (used when MODE='development')
VITE_RAZORPAY_TEST_KEY_ID=rzp_test_xxxxx
# Production key (used when MODE='production')
VITE_RAZORPAY_KEY_ID=rzp_live_xxxxx

# Stripe Configuration (for USD payments)
# Development key (used when MODE='development')
VITE_STRIPE_TEST_PUBLISHABLE_KEY=pk_test_xxxxx
# Production key (used when MODE='production')
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_xxxxx

# Application Settings
VITE_APP_NAME=GEMA Events
VITE_APP_URL=http://localhost:5173

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
